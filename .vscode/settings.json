{
  "files.exclude": {
    "node_modules/": true,
    "package-lock.json": true,
    "composer.lock": true,
    "Dockerfile": true,
    ".github": false,
    ".git": false,
    ".gitignore": false,
    "captain-definition": true,
    "vite.config.js": true,
    "backend/vendor": true,
    "*/dist": true,
    "package.json": false,
    "composer.json": false,
    "ts*.json": true,
    "tmp/*": true,
    "frontend/foundation/": true,
    "frontend/mixins/": true,
    "frontend/utils/": true,
},
"vue3snippets.enable-compile-vue-file-on-did-save-code": false,
"cSpell.diagnosticLevel": "Hint",
}
